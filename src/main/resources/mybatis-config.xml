		<typeAlias alias="BulletinPublishRangeInst" type="com.cattsoft.baseplatform.func.sm.entity.BulletinPublishRangeInst" />
		<typeAlias alias="BulletinFeedback" type="com.cattsoft.baseplatform.func.sm.entity.BulletinFeedback" />
		<typeAlias alias="UserPageSetting" type="com.cattsoft.baseplatform.func.sm.entity.UserPageSetting" />
		<typeAlias alias="PageLayout" type="com.cattsoft.baseplatform.func.sm.entity.PageLayout" />
		<typeAlias alias="PageComponent" type="com.cattsoft.baseplatform.func.sm.entity.PageComponent" />
		<typeAlias alias="Unit" type="com.cattsoft.baseplatform.func.sm.entity.Unit" />
		<typeAlias alias="SysMsg" type="com.cattsoft.baseplatform.func.sm.entity.SysMsg" />	    
